{
  "manifest_version": 2,
  "name": "Pokechrome",
  "description": "Pokedex Chrome Extension that identifies web pages and utilizes PokeAPI.",
  "version": "0.1.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Pokechrome"
  },
  "icons": {
    "128": "pokeball64.png"
  },
  "background": {
    "scripts": ["background.ts"]
  },
  "permissions": ["tabs", "activeTab", "storage", "background"]
}
